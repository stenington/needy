pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function near(a,b)
 return abs(a.x-b.x)<8
  and abs(a.y-b.y)<8
end

function moveto(a,b)
 diffx=b.x-a.x
 if diffx!=0 then
  dx=(diffx/abs(diffx))*min(abs(diffx),1)
  a.x+=dx
 end
 diffy=b.y-a.y
 if diffy!=0 then
  dy=(diffy/abs(diffy))*min(abs(diffy),1)
  a.y+=dy
 end
end


waiting={}

waiting.update = function(guy)
 guy.hungry+=1
 guy.sleepy+=1
 if guy.hungry>1000 then
  guy.switchto(eating)
 elseif guy.sleepy>1000 then
  guy.switchto(sleeping)
 elseif tick%30==0 then
  guy.x+=flr(rnd(3))-1
  guy.y+=flr(rnd(3))-1
 end
end
 
waiting.draw = function(guy)
 spr(1,guy.x,guy.y)
end


eating={}

eating.update = function(guy)
 if near(guy, food) then
  eating.act(guy)
 else
  guy.hungry+=1
  guy.sleepy+=1
  moveto(guy,food)
 end
end
 
eating.draw = function(guy)
 spr(2,guy.x,guy.y)
end

eating.act = function(guy)
 guy.sleepy-=1
 guy.hungry-=5
 if guy.hungry<=0 then
  guy.switchto(waiting)
 end
end

eating.finish = function()
 food.x = flr(rnd(120))
 food.y = flr(rnd(120))
end


sleeping={}

sleeping.update = function(guy)
 if near(guy,bed) then
  sleeping.act(guy)
 else
  guy.hungry+=1
  guy.sleepy+=1
  moveto(guy,bed)
 end
end

sleeping.act = function(guy)
 guy.hungry+=2
 guy.sleepy-=2
 if guy.sleepy<=0 then
  guy.switchto(waiting)
 end
end

sleeping.draw = function(guy)
 spr(3,guy.x,guy.y)
end

sleeping.finish = function()
 bed.x = flr(rnd(120))
 bed.y = flr(rnd(120))
end


guy={}

guy.switchto = function(next)
 curr=guy.state
 if curr.finish then
  curr.finish()
 end
 guy.state=next
 if next.start then
  next.start()
 end
end


bed={}

bed.draw = function(self)
 spr(16,self.x,self.y)
end


food={}

food.draw = function(self)
 spr(17,self.x,self.y)
end


function _init()
 tick=0
 
 guy.x=60
 guy.y=60
 guy.hungry=0
 guy.sleepy=0
 guy.state=waiting
 
 bed.x=30
 bed.y=60
 
 food.x=90
 food.y=60
 
 ents={bed,food}
end

function _update()
 tick+=1
 guy.state.update(guy)
 for ent in all(ents) do
  if ent.update then
   ent.update()
  end
 end
end

function _draw()
 cls()
 for ent in all(ents) do
  ent.draw(ent)
 end
 guy.state.draw(guy)
 rectfill(4,4,106,6,5)
 line(5,5,(guy.hungry/10)+5,5,10)
 rectfill(4,9,106,11,5)
 line(5,10,(guy.sleepy/10)+5,10,11)
end
