pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function near(a,b)
 return abs(a.x-b.x)<8
  and abs(a.y-b.y)<8
end

function moveto(a,b)
 diffx=b.x-a.x
 dx=0
 dy=0
 if diffx!=0 then
  dx=(diffx/abs(diffx))*min(abs(diffx),1)
 end
 diffy=b.y-a.y
 if diffy!=0 then
  dy=(diffy/abs(diffy))*min(abs(diffy),1)
 end
 a.x+=dx
 a.y+=dy
 if dx<0 then a.flip=true end
 if dx>0 then a.flip=false end
end

function graph(val,minv,maxv,y,col,ticks)
 span=maxv-minv
 zerop=13+(-minv/span*100)
 linex=zerop+(val/span*100)
 rectfill(13,y-1,115,y+1,5)
 for t in all(ticks) do
  tx=zerop+(t/span*100)
  line(tx,y-1,tx,y+1,6)
 end
 line(zerop,y,linex,y,col)
end


waiting={}

waiting.update = function(guy)
 guy.hungry+=1
 guy.sleepy+=1
 if guy.hungry>100 then
  guy.switchto(eating)
 elseif guy.sleepy>100 then
  guy.switchto(sleeping)
 elseif tick%30==0 then
 	dx=flr(rnd(3))-1
  dy=flr(rnd(3))-1
  moveto(guy,{x=guy.x+dx,y=guy.y+dy})
 end
end
 
waiting.draw = function(guy)
 spr(1,guy.x,guy.y,1,1,guy.flip)
end


eating={}

eating.update = function(guy)
 if near(guy, food) then
  eating.act(guy)
 else
  guy.hungry+=1
  guy.sleepy+=1
  moveto(guy,food)
 end
end
 
eating.draw = function(guy)
 spr(1,guy.x,guy.y,1,1,guy.flip)
end

eating.act = function(guy)
 guy.sleepy-=3
 guy.hungry-=8
 if guy.hungry<=0 then
  guy.switchto(waiting)
 end
end

eating.finish = function()
 food.x = flr(rnd(120))
 food.y = flr(rnd(120))
end


sleeping={}
sleeping.asleep=false

sleeping.update = function(guy)
 if near(guy,bed) then
  sleeping.asleep=true
  sleeping.act(guy)
 else
  sleeping.asleep=false
  guy.hungry+=1
  guy.sleepy+=1
  moveto(guy,bed)
 end
end

sleeping.act = function(guy)
 guy.hungry+=2
 guy.sleepy-=2
 if guy.sleepy<=0 then
  guy.switchto(waiting)
 end
end

sleeping.draw = function(guy)
 if sleeping.asleep then
  spr(2,guy.x,guy.y,1,1,guy.flip)
 else
  spr(1,guy.x,guy.y,1,1,guy.flip)
 end
end

sleeping.finish = function()
 -- nuthin
end


guy={}
guy.flip=false

guy.switchto = function(next)
 curr=guy.state
 if curr.finish then
  curr.finish()
 end
 guy.state=next
 if next.start then
  next.start()
 end
end


bed={}

bed.draw = function(self)
 spr(16,self.x,self.y)
end


food={}

food.draw = function(self)
 spr(17,self.x,self.y)
end


function _init()
 tick=0
 
 guy.x=60
 guy.y=60
 guy.hungry=0
 guy.sleepy=0
 guy.state=waiting
 
 bed.x=30
 bed.y=60
 
 food.x=90
 food.y=60
 
 ents={bed,food}
end

function _update()
 tick+=1
 guy.state.update(guy)
 for ent in all(ents) do
  if ent.update then
   ent.update()
  end
 end
end

function _draw()
 cls()
 for ent in all(ents) do
  ent.draw(ent)
 end
 guy.state.draw(guy)
 
 graph(guy.hungry,-100,300,5,10,{0,100})
 graph(guy.sleepy,-100,300,10,11,{0,100})
end
